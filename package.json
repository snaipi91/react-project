    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "react-router-redux": "^4.0.5",
    "react-select": "^1.0.0-beta14",
    "react-skylight": "^0.4.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "yandex-map-react": "^1.0.0-beta.12"
  },
