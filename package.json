    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.11.6",
    "body-parser": "^1.15.2",
    "cheerio": "^0.20.0",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
